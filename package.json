{
  "name": "notius-server",
  "version": "0.0.1",
  "description": "a Node.js server for DCS:WORLD and Notius-MMS",
  "main": "webpack.config.js",
  "dependencies": {
    "express": "^4.15.2",
    "fs": "^0.0.1-security",
