{
  "name": "notius-server",
  "version": "0.0.1",
  "description": "a Node.js server for DCS:WORLD and Notius-MMS",
  "private": true,
  "dependencies": {
    "express": "^4.15.2",
    "fs": "^0.0.1-security",
    "nodejs-websocket": "^1.7.1",
    "path": "^0.12.7",
    "rimraf": "^2.6.1"
  },
  "scripts": {
    "start": "npm run build:min && node ./build/notius-server.min.js",
    "pack": "webpack --watch -p --progress --profile --colors",
    "build:dev": "babel ./src --out-dir dev --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files ",
    "build:min": "npm run build && npm run pack",
    "dev": "npm run build:dev && node dev/notius-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jkhoel/notius-server.git"
  },
  "author": "Jens Kristian Hoel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkhoel/notius-server/issues"
  },
  "homepage": "https://github.com/jkhoel/notius-server",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "webpack": "^2.7.0",
    "webpack-cli": "^3.3.5"
  }
}
