{
  "name": "notius-server",
  "version": "0.0.1",
  "description": "a Node.js server for DCS:WORLD and Notius-MMS",
  "main": "webpack.config.js",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "express": "^4.15.2",
    "fs": "^0.0.1-security",
    "nodejs-websocket": "^1.7.1",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1"
